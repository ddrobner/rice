URxvt*scrollBar: false

Xft*dpi: 144
URxvt.font:	xft:Hack-Regular:pixelsize=14,xft:Symbola

! X colors.
! Generated by 'wal'
*foreground:        #d4d9de
*background:        #0d0f13
*.foreground:       #d4d9de
*.background:       #0d0f13
emacs*foreground:   #d4d9de
emacs*background:   #0d0f13
URxvt*foreground:   #d4d9de
XTerm*foreground:   #d4d9de
UXTerm*foreground:  #d4d9de
URxvt*background:   [80]#0d0f13
XTerm*background:   #0d0f13
UXTerm*background:  #0d0f13
URxvt*cursorColor:  #d4d9de
XTerm*cursorColor:  #d4d9de
UXTerm*cursorColor: #d4d9de
URxvt*borderColor:  [100]#0d0f13

! Colors 0-15.
*.color0: #0d0f13
*color0:  #0d0f13
*.color1: #60748B
*color1:  #60748B
*.color2: #758A9F
*color2:  #758A9F
*.color3: #6598D0
*color3:  #6598D0
*.color4: #99A4AF
*color4:  #99A4AF
*.color5: #C4C0B9
*color5:  #C4C0B9
*.color6: #A9B8C6
*color6:  #A9B8C6
*.color7: #d4d9de
*color7:  #d4d9de
*.color8: #94979b
*color8:  #94979b
*.color9: #60748B
*color9:  #60748B
*.color10: #758A9F
*color10:  #758A9F
*.color11: #6598D0
*color11:  #6598D0
*.color12: #99A4AF
*color12:  #99A4AF
*.color13: #C4C0B9
*color13:  #C4C0B9
*.color14: #A9B8C6
*color14:  #A9B8C6
*.color15: #d4d9de
*color15:  #d4d9de

! Black color that will not be affected by bold highlighting.
*.color66: #0d0f13
*color66:  #0d0f13

! Xclock colors.
XClock*foreground: #d4d9de
XClock*background: #0d0f13
XClock*majorColor:  rgba:d4/d9/de/ff
XClock*minorColor:  rgba:d4/d9/de/ff
XClock*hourColor:   rgba:d4/d9/de/ff
XClock*minuteColor: rgba:d4/d9/de/ff
XClock*secondColor: rgba:d4/d9/de/ff

! Set depth to make transparency work.
URxvt*depth: 32

